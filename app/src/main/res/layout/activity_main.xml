                    android:src="@drawable/sun_rise_icon"
                    app:layout_constraintBottom_toTopOf="@+id/sun_set_icon"
                    app:layout_constraintEnd_toStartOf="@+id/sun_rise_text"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
