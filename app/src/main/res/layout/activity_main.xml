                    app:layout_constraintEnd_toStartOf="@+id/sun_set_text"
                    app:layout_constraintStart_toEndOf="@+id/weather0_icon" />

                <TextView
                    android:id="@+id/sun_rise_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="40dp"
                    android:layout_marginEnd="48dp"
                    android:layout_marginBottom="16dp"
                    android:text="@string/_5_00"
                    android:textColor="@color/primary_text_color"
                    android:textSize="18sp"
                    app:layout_constraintBottom_toTopOf="@+id/sun_set_text"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="TextContrastCheck" />

                <ImageView
                    android:id="@+id/humidity_icon"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:contentDescription="@string/todo"
                    android:layout_marginStart="22dp"
                    android:layout_marginEnd="14dp"
                    android:src="@drawable/humidity_icon"
                    app:layout_constraintBottom_toTopOf="@+id/windy0_icon"
                    app:layout_constraintEnd_toStartOf="@+id/humidity_text"
                    app:layout_constraintStart_toEndOf="@+id/weather0_icon" />

                <TextView
                    android:id="@+id/windy_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="6dp"
                    android:layout_marginBottom="12dp"
                    android:text="@string/_70_5km_h"
                    android:textColor="@color/primary_text_color"
                    android:textSize="18sp"
                    app:layout_constraintBottom_toTopOf="@+id/air_pressure_text"
                    app:layout_constraintEnd_toEndOf="parent"
                    tools:ignore="TextContrastCheck" />

                <ImageView
                    android:id="@+id/windy0_icon"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="22dp"
                    android:contentDescription="@string/todo"
                    android:layout_marginEnd="14dp"
